{
    "version": 2,
    "buildCommand": "mkdir -p assets/build && pnpm install --no-frozen-lockfile && pnpm run build && pnpm run build:docs",
    "outputDirectory": "dist",
    "installCommand": "pnpm install --no-frozen-lockfile",
    "framework": null,
    "functions": {
        "api/vercel.ts": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/vercel.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "CACHE_TYPE": "memory",
        "CACHE_EXPIRE": "300",
        "CACHE_CONTENT_EXPIRE": "3600"
    }
} 
