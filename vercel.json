{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/node",
            "config": {
                "buildCommand": "pnpm install && pnpm run build",
                "outputDirectory": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.js"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "CACHE_TYPE": "memory",
        "CACHE_EXPIRE": "300",
        "CACHE_CONTENT_EXPIRE": "3600"
    }
} 
