{
    "version": 2,
    "builds": [
        {
            "src": "dist/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["dist/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ],
    "buildCommand": "pnpm install && pnpm run build",
    "outputDirectory": "dist",
    "env": {
        "NODE_ENV": "production",
        "CACHE_TYPE": "memory",
        "CACHE_EXPIRE": "300",
        "CACHE_CONTENT_EXPIRE": "3600"
    }
} 
