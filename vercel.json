{
    "version": 2,
    "buildCommand": "mkdir -p assets/build && pnpm install --no-frozen-lockfile && pnpm run build && cp -r lib .",
    "outputDirectory": ".",
    "installCommand": "pnpm install --no-frozen-lockfile",
    "framework": null,
    "functions": {
        "api/vercel.ts": {
            "memory": 1024,
            "maxDuration": 60
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/vercel",
            "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
                "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
            }
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "CACHE_TYPE": "memory",
        "CACHE_EXPIRE": "300",
        "CACHE_CONTENT_EXPIRE": "3600"
    }
} 
